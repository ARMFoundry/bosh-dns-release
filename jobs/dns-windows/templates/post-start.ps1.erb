$ErrorActionPreference = "Stop"

try
{
    Resolve-DnsName -DnsOnly -Name healthcheck.bosh-dns. -Server <%= p('address').to_s == "0.0.0.0" ? "127.0.0.1" : p('address') %>
    Resolve-DnsName -TcpOnly -DnsOnly -Name healthcheck.bosh-dns. -Server <%= p('address').to_s == "0.0.0.0" ? "127.0.0.1" : p('address') %>
}
catch
{
    $Host.UI.WriteErrorLine($_.Exception.Message)
    Exit 1
}
Exit 0
